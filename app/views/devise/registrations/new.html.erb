<div class="container" id="panel">

<h2 class="form-signin-heading">Novo Cadastro</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <div><%= devise_error_messages! %></div>

  <div class="md-form">
  <%= f.text_field :usuario,:id=>"inputMDEx" ,:class =>"form-control"%>
    <%= f.label :usuario,:for=>"inputMDEx" %>
  </div>

  <div class="md-form">
  <%= f.email_field :email,:id=>"inputMDEx" ,:class =>"form-control"%>
    <%= f.label :email,:for=>"inputMDEx" %>
  </div>

  <div class="md-form">
    <%= f.label :password,:for=>"inputMDEx" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> caracteres no m√≠nimo)</em>
    <% end %><br />
    <%= f.password_field :password,:id=>"inputMDEx" ,:class =>"form-control"%>
  </div>



  <div class="md-form">
    <%= f.label :password_confirmation,:for=>"inputMDEx" %>
    <%= f.password_field :password_confirmation, :id=>"inputMDEx" ,:class =>"form-control" %>
  </div>

  <div class="actions">
    <%= f.submit "Cadastrar", class:"btn btn-md btn-deep-purple" %>
  </div>
<% end %>

<%= render "devise/shared/links" %>

</div> <!-- /container -->
