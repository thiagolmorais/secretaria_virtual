<body class="app animated fadeInUpBig">
  <i id="icon" class="fa fa-group"></i>
  <div class="container" id="panel">
    <h2>Login</h2>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <div class="md-form">
      <%= f.email_field :email,:id=>"inputMDEx" ,:class =>"form-control"%>
      <%= f.label :email,:for=>"inputMDEx" %>
    </div><br>

    <div class="md-form">
      <%= f.password_field :password,:id=>"inputMDEx1" ,:class =>"form-control"%>
      <%= f.label :password,:for=>"inputMDEx1" %>
    </div>
    <% if devise_mapping.rememberable? -%><br>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me , :class=>"lembrar"%>
      </div>
    <% end -%>
    <div class="actions">
      <%= f.submit "Entrar", class:"btn btn-sm btn-deep-purple" %><%= render "devise/shared/links" %>
    </div>

  <% end %>

</div>

</body>
